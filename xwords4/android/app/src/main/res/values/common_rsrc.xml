  <!-- Triples of Name, supported codes, and URL format string -->
  <string-array name="lookup_urls">
    <!--  -->
    <item>Dictionary.com</item>
    <item>:en:</item>
    <item>http://dictionary.com/browse/%2$s</item> -->
    <!--  -->
    <item>TheFreeDictionary.com</item>
    <item>:en:es:de:it:nl:pt:pl:</item>
    <item>http://%1$s.thefreedictionary.com/%2$s</item>
    <!--  -->
    <item>Larousse.com</item>
    <item>:fr:</item>
    <item>http://www.larousse.com/%1$s/dictionaries/french/%2$s</item>
    <!--  -->
    <item>Littre.reverso.net</item>
    <item>:fr:</item>
    <item>http://littre.reverso.net/dictionnaire-francais/definition/%2$s</item>
    <!--  -->
    <item>DIEC</item>
    <item>:ca:</item>
    <item>http://diccionari.totescrable.cat/cercador/?mot=%2$s</item>
    <!--  -->
    <item>Viccionari</item>
    <item>:ca:</item>
    <item>http://%1$s.wiktionary.org/w/index.php?search=%2$s</item>
    <!--  -->
    <item>Wiktionnaire</item>
    <item>:fr:</item>
    <item>http://%1$s.wiktionary.org/w/index.php?search=%2$s</item>
    <!--  -->
    <item>DISC lookup</item>
    <item>:ca:</item>
    <item>http://escrable.montane.cat/diccionari/cercador/?mot=%2$s</item>
    <!--  -->
    <item>Wiktionary</item>
    <item></item>
    <item>http://%1$s.wiktionary.org/w/index.php?search=%2$s</item>
    <!--  -->
    <item>Google</item>
    <item></item> <!-- means all supported -->
    <item>http://www.google.com/search?nl=%1$s\u0026q=%2$s</item>
  </string-array>

